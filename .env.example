# Configuración para STT con Groq
# Copia este archivo como .env y configura tus valores

GROQ_API_KEY=tu_clave_api_groq_aqui

# Configuración opcional avanzada
GROQ_STT_VAD_AGGRESSIVENESS=2
GROQ_STT_MIN_SECONDS=10
GROQ_STT_MAX_SECONDS=30
GROQ_STT_SILENCE_THRESHOLD=1.0
GROQ_STT_PROMPT=terminología educativa, prótesis dental, microbiología clínica
GROQ_STT_DEBUG=false